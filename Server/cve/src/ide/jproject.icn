   #
   # saveJProject
   #
   method saveJProject(pname)
   local i, index := 0, s, f

      projectCreated :="TRUE"
      every i := *pname-1 to 0 do
	  if any('./\\', pname[i]) then {
	     index := i
	     break
	     }
      if pname[index] ~== "." then	# user didn't supply extension
	     pname ||:= ".uprj"

      #where to write project file info.
      if f := open(filedirectory||PS||file_name(pname)||".uprj", "w") then {
         write(f, s:="#"||repl("=", 55))
         writes(f, "#@V@:Note: Java Project File generated by CVE IDE  ")
         write(f, UIDE_VERS, " (", \compilerName.get_contents(), ").")
         write(f, "# Generated ", &clock, " ", &date)
         write(f, s)
         write(f, "\nProject Name    : ", pname)
         write(f, "Files Directory : ", filedirectory)
         write(f, "\n//% Section 1 - COMPILER")
         if (/compilerName.get_contents()) then
            write(f, "<none>\n")
         write(f, compilerName.get_contents(), "\n")

         write(f, "//% Section 2 - COMPILER OPTIONS")
         every write(f, !compOptsPoolList)

         write(f, "//%compiler-opts-pool-end")
         every write(f, !compOpts)

         write(f, "//%compiler-opts-end")

         write(f, "//% Section 3 - INTERPRETER")
         if /interpName.get_contents() then
            write(f, "<none>\n")
         write(f, interpName.get_contents(), "\n")

         write(f, "//% Section 4 - INTERPRETER OPTIONS")
         every write(f, !interpOptsPoolList)
         write(f, "//%interp-opts-pool-end")

         every write(f, !interpOpts)

         write(f, "//%interp-opts-end\n")

         write(f, "//% Section 5 - DEBUGGER")
	 if \debugName.get_contents() then
	    write(f, "<none>\n")
         write(f, debugName.get_contents(), "\n")

         write(f, "//% Section 6 - CLASSPATH")
         if \classpath.get_contents() then
            write(f, "<none>\n")
         write(f, classpath.get_contents(), "\n")

         write(f, "//% Section 7 - APPLET HTML SORUCE")
         if \appletHTML.get_contents() then
            write(f, "<none>\n")
         write(f, appletHTML.get_contents(), "\n")

         write(f, "//% Section 8 - TOP LEVEL JAVA FILE")
         if \mainFile.get_contents() then
            write(f, "<none>\n")
         write(f, mainFile.get_contents(), "\n")

         write(f, "//% Section 9 - SOURCE DIRECTORY")
         if \srcDir.get_contents() then
            write(f, "<none>\n")
         write(f, srcDir.get_contents(), "\n")

         write(f, "//% Section 10 - SOURCE FILES")
         #every ix := 1  to *srcFiles do
         #   write(f, srcFiles[ix])
         write(f, "//%src-files-end")

         write(f, "//% Section 11 - SOURCE DIRECTORY")
         if \ObjDir.get_contents() then
            write(f, "<none>\n")
         write(f, ObjDir.get_contents(), "\n")
         close(f)
         }
      if projecttype == "applet" then {
         if f := open(filedirectory||PS||
            file_name(pname)||".html", "w") then {
            write(f, "<applet width=300 height=300 code="||
                      file_name(pname)||".class"||"> </applet>")
            close(f)
            }
         }
   end
   #
   # CompileJava
   #
   method CompileJava()
      local cmd, makeFile, index := 0, sdLen, cmdLen, lst:=[], n,
         nmakeFile, n_ext, pdir

       mkpath := mFile

       if /mkpath then {
          tab_label ? {
             nmakeFile := tab(find("."))
             move(1)
             n_ext := tab(0)
             }

          cmd := "javac "||nmakeFile ||".java" # javac + FILENAME.java

          pdir := chdir()

          chdir(pdir)

$ifdef _UNIX
          system(cmd ||:= " > "||"output.txt"||" 2>&1")
          view.uide.showanyerror("output.txt", lst)
$else
          system(cmd) #"cmd /C "||cmd||" > "||"output.txt"||" 2>&1")
          #view.uide.showanyerror("output.txt", lst)
$endif
          }
      else {
         # We will either be running off a Java file specified only as
         # a Makefile, or via the attributes in the project file.

         if /mkpath then {
            nullfilename := &window
            &window  := &null
               Notice("No Java File specified.")
            &window := nullfilename
            self.dispose()
            }

        if /find(".java", mkpath) then  #no .java
           mkpath := mkpath ||".java"

        # Now, change directory
        cmd := mkpath
        n := *cmd
        every ix := 1 to n do {
           if cmd[ix] == ("\\" | "/") then {
$ifdef _UNIX
              cmd[ix] := "/"
$else
              cmd[ix] := "\\"
$endif
              index := ix
              break
              }
           }
        if index > 1 then {
           makeFile := cmd[1:*cmd+1]	  # copy the makefile name
           chdir(cmd)      # change dir
           }
        else makeFile := cmd

        #@@ java compiler + switches...

        # rm as many .class files as I can find - this is because javac
        # really doesn't do dependencies, and it is too hard to do it
        # ourselves, so we will just kill them all.
        #

        cmd :="rm "
        if (\obj_dir & (obj_dir ~=="<none>")) |
           ((*src_dir >1) & (src_dir ~=="<none>")) then {
           cmd ||:= "-r " || obj_dir || "/*.class "
           }
        else if *src_dir > 1 & src_dir ~=="<none>" then {
           cmd ||:= "-r " || obj_dir || "/*.class"
           }
        else cmd ||:= filedirectory||PS||"*.class"

        chdir(filedirectory)
        system(cmd)

        cmd := comp_name #make or make clean passed from caller

        if (\cpath &
           (cpath ~=="<none>")) then {
           cmd ||:= " -classpath " || cpath
           }

        every cmd ||:= " " || !compOpts	# add switches
        cmd ||:= " "

        if (\obj_dir & (obj_dir ~=="<none>")) then {
           cmd ||:= "-d " || "/" || " " #obj_dir
           }

        # Check for src dir

        sdLen := *src_dir
        if (sdLen > 0 & src_dir ~==".") & (src_dir ~== "<none>") then {
           cmd ||:= "-sourcepath " || src_dir || " " || src_dir
           if src_dir[sdLen-1] ~== "/" & src_dir[sdLen-1] ~== "\\" then
	          cmd ||:= "/"
           }

        tab_label ? {
	       nmakeFile := tab(find("."))
		   move(1)
		   n_ext := tab(0)
	       }

        cmd ||:= nmakeFile ||".java" # javac + FILENAME.java

        chdir(pdir)

$ifdef _UNIX
        system(cmd ||:= " > "||"output.txt"||" 2>&1")
        view.uide.showanyerror("output.txt", lst)
$else
        system(cmd) #"cmd /C "||cmd||" > "||"output.txt"||" 2>&1")
        #view.uide.showanyerror("output.txt", lst)
$endif
        }
  end

  #
  # RunJava
  #
  method RunJava(javaprog) #javaprog is mFile
     local runit, path, prog, curPath, buff, lst:=[], cwd
     cwd := chdir()

     needConsole := "FALSE"

     if /runit then {
        if JavaType == "ConsoleApp" then {
           javaprog := tab_label[1:*tab_label-4]
           runit := "java"
           openrunit := "java"||" "||javaprog
           }
        else if JavaType == "WinApp" then {
           javaprog := tab_label[1:*tab_label-4]
           runit := "javaw"
           openrunit := "javaw"||" "||javaprog
           }
        else if JavaType == "Applet" then {
           applet_html := tab_label[1:*tab_label-4]||".html"
           runit := "appletviewer"
           openrunit := "appletviewer"||" "||applet_html
           }

$ifdef _UNIX
        if find("javaw", runit) then    # GUI - assume no console
           runit :="java"              # Unix only has java, no javaw
        else
           needConsole := "TRUE"
$else
        if runit == "java" then   # console app?
           needConsole := "TRUE"
$endif

        UideShell :="xterm" # depends on the OS
        if needConsole == "TRUE" then {
$ifdef _UNIX
           buff := UideShell || " -e \"" || runit || "\""
           system(openrunit || " &> " || "output.txt" || " 2>&1")
           view.uide.showanyerror("output.txt", lst)
$else
           system(openrunit) #"cmd /C "||openrunit|| " &> " || "output.txt" || " 2>&1")
           #view.uide.showanyerror("output.txt", lst)
$endif
           }
        }
     else {
        runit:= interp_name   # use interp provided
$ifdef _UNIX
        if find("javaw", runit) then    # GUI - assume no console
           runit :="java"              # Unix only has java, no javaw
        else
           needConsole := "TRUE"
$else
        if runit == "java" then   # console app?
           needConsole := "TRUE"
$endif

        #@@ Probably need to do something completely different
        #@@ for HTML applets and appletviewer applets - like
        #@@ launch the browser if HTML supplied, otherwise
        #@@ use the viewer.

        if (/applet_html) | (applet_html == "<none>") then
            prog := javaprog	# a Java app
        else {
           runit := "appletviewer"  # use interp provided
           prog := applet_html # an applet .html file
           }

        every ix:= *javaprog-1 to 1 do {
           if javaprog[ix] == ("\\" | "/") then {
              path := javaprog
              prog := path[ix+1 : *javaprog] # this will be the prog
              path[ix] := 0			# and zap the prog
              break
              }
           }

        if (\class_path & \((class_path) =="<none>") &
           \(runit == "appletviewer")) then {
           runit ||:= " -cp " || class_path
           }

        if \interpOpts then {
           every runit ||:=" " || !interpOpts # add switches
           }

        runit ||:= " " || file_name(prog)

        if (\obj_dir & (obj_dir ~=="<none>")) then {
           chdir(obj_dir)	# change dir
           }
        else if (*(src_dir) > 1 & src_dir ~=="<none>") then {
           chdir(src_dir)	# change dir
           }

        UideShell :="xterm" # depends on the OS
        if needConsole == "TRUE" then {
$ifdef _UNIX
           buff := UideShell || " -e \"" || runit || "\""
           system(runit || " &> " || "output.txt" || " 2>&1")
           view.uide.showanyerror("output.txt", lst)
$else
           system(openrunit) #"cmd /C "||runit|| " &> " || "output.txt" || " 2>&1")
           #view.uide.showanyerror("output.txt", lst)
$endif
           }
        }
        chdir(cwd)
  end

  method on_addToCompList(ev)
     local L := compOptsPool.get_contents(),
         cop := compOptsPool.get_cursor()
     if L[cop] == "-g (Full Debugging Info)" then
        put(compOpts, "-g")
     else if L[cop] == "-g:none (No Debugging Info)" then
	put(compOpts, "-g:none")
     else if L[cop] == "-O (Optimize compiled code)" then
	put(compOpts, "-O")
     else if L[cop] == "-nowarn (No warnings)" then
	put(compOpts, "-nowarn")
     else if L[cop] == "-verbose (more messages)" then
	put(compOpts, "-verbose")
     else if L[cop] =="-deprecation (Deprecated API)" then
	put(compOpts, "-deprecation")
     usedcompOptsPool.set_contents(compOpts)
   end

   method on_addToInterpList(ev)
      local L := interpOptsPool.get_contents(),
         iop := interpOptsPool.get_cursor()
      if L[iop] == "-verbose (more output)" then
	 put(interpOpts, "-verbose")
      else put(interpOpts, L[iop])
      usedinterpOptsPool.set_contents(interpOpts)
   end

