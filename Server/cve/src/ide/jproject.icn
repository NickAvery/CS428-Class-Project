      interp_tab.add(delinterpreter_btn)
      editinterpreter_btn := TextButton()
      editinterpreter_btn.set_pos(98, 213)
      editinterpreter_btn.set_size(41, 25)
      editinterpreter_btn.clear_toggles()
      editinterpreter_btn.set_label("Edit")
      editinterpreter_btn.set_internal_alignment("c")
      interp_tab.add(editinterpreter_btn)
      JProjectTabSet.add(interp_tab)
      debug_tab := TabItem()
      debug_tab.set_label("Debugger")
      label_12 := Label()
      label_12.set_pos(17, 25)
      label_12.set_size(97, 25)
      label_12.set_internal_alignment("l")
      label_12.set_label("Java Debugger:")
      debug_tab.add(label_12)
      debugName := TextField()
      debugName.set_pos(114, 24)
      debugName.set_size(290, 25)
      debugName.set_draw_border()
      debugName.set_contents("")
