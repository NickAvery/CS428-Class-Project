   method init_dialog()
      local pname, pext
      #set project defaults
      compilerName.set_contents("javac")
      interpName.set_contents("java")
      debugName.set_contents("jdb")

      if find(".uprj"|".icn"|".java", projectname) then {
	 projectname ? {
	    pname := tab(find("."))
	    move(1)
	    pext := tab(0)
	    }
	 projectname := pname
	 }

      mainFile.set_contents(file_name(projectname))
      appletHTML.set_contents("<none>")
      classpath.set_contents("<none>")
      srcDir.set_contents(".")
      ObjDir.set_contents("<none>")

      compOptsPoolList :=["-g (Full Debugging Info)",
		  	   "-g:none (No Debugging Info)",
		  	   "-O (Optimize compiled code)",
		  	   "-nowarn (No warnings)",
		  	   "-verbose (more messages)",
		  	   "-deprecation (Deprecated API)"]
      interpOptsPoolList :=["-verbose (more output)"]
      compOptsPool.set_contents(compOptsPoolList)
      interpOptsPool.set_contents(interpOptsPoolList)
      case (projecttype) of {
	 "applet": {
	    appletHTML.set_contents(mainFile.get_contents() ||".html")
	    interpName.set_contents("appletviewer")
	    debugName.set_contents("appletviewer -debug")
	    }
	 "winApp": {
	    interpName.set_contents("javaw")

	    }
	 "consoleApp": {
            needConsole := "TRUE"
	    }
	}
      compOpts :=[]
      interpOpts :=[]
   end

