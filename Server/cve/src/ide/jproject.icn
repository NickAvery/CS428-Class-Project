      ok_btn := TextButton()
      ok_btn.set_pos("4", "272")
      ok_btn.set_size("65", "24")
      ok_btn.connect(self, "on_ok_btn", ACTION_EVENT)
      ok_btn.clear_toggles()
      ok_btn.set_label("OK")
      ok_btn.set_internal_alignment("c")
      self.add(ok_btn)
