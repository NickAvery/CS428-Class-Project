   method init_dialog()
      local q , qn, d
      
      if /quest then {
         write("Error: No Quest to Show!")
         dispose()
         }
      
      titlelbl.set_label(\(quest.title) || " Quest"  |"Quest Screen")
      
      if quest.which_type=="short answer" then{
         if quest.set_quest_source("quiz") then
	    init_short_answer_quest()
	 }
      else if quest.which_type=="java" then
         init_java_quest()
      
      user_choice := "cancel"

      self.init()
      self.resize()
   end
   
   method init_MC(ans)
      local L, i, size
      
      L := quest.get_current_question_answer_list()
      if /L then fail 

      if map(L[1])== ("true" | "false" | "yes" | "no") | 
	 map(L[2])== ("true" | "false" | "yes" | "no") then{
	 size := 2
	 type_lst.set_selection(3)	# True / False
	 }
      else{
	 size := 4
	 type_lst.set_selection(2)	# Multiple Choice
	 }
      
      every i:=1 to size do{
	 #chk_list[i].set_label(L[i])
	 mc_list[i].set_contents([L[i]])
	 visible_set_add( chk_list[i])
	 visible_set_add( mc_list[i])
	 }
	 
      every i:=size+1 to 4 do{
	 visible_set_remove( chk_list[i])
	 visible_set_remove( mc_list[i])
	 mc_list[i].set_contents( [""] )
	 }
	 

      if /ans then
	 check_box_group_1.set_which_one(chktmp)
      else if  0 < ans <= *L then
	 check_box_group_1.set_which_one(chk_list[ans])
      else
	 check_box_group_1.set_which_one(chktmp)

   end
   
   method visible_set_add(obj, from)
      if member(visible_set, obj) then return
      /from := border_option_list
       from.add(obj)
       insert(visible_set, obj)
   end
   
   method visible_set_remove(obj, from)
      if not(member(visible_set, obj)) then return
      /from := border_option_list
       from.remove(obj)
       delete(visible_set, obj)
   end
   
   method init_short_answer_quest()
   local q, qn, d
      q := quest.get_current_question() | quest.get_next_question()
	 
      if /q then{
         questiontl.set_contents([""])
	 quest.add_question_answer(QuestionAnswer("",""))
	 q := quest.get_current_question() | quest.get_next_question()
      }
      else
	 questiontl.set_contents([q])
      
      q := quest.get_answer()
      
      visible_set := set( chk1, chk2, chk3, chk4, mc1_etl, mc2_etl, mc3_etl, mc4_etl )
      
      chktmp := CheckBox()
      chktmp.set_pos("1000", "1000")
      chktmp.set_size(2, 2)
      chktmp.set_toggles()
      
      check_box_group_1.add(chktmp)
      
      chk_list := []
      put(chk_list, chk1)
      put(chk_list, chk2)
      put(chk_list, chk3)
      put(chk_list, chk4)      
      
      mc_list := []
      put(mc_list, mc1_etl)
      put(mc_list, mc2_etl)
      put(mc_list, mc3_etl)
      put(mc_list, mc4_etl)      
      
      
      if quest.is_current_question_MC() then{
	 #visible_set_remove(answeretl)
	 #visible_set_add(border_option_list)
	 overlay_set.set_which_one(overlay_item_MC)
	 init_MC(q)
      }
      else{
	 #visible_set_remove(border_option_list)
	 #visible_set_add(answeretl)
	 overlay_set.set_which_one(overlay_item_short)
	 if \q then
	    answeretl.set_contents([q])
      }
            
      qn := quest.question_count()
      if qn>0 then
         progress_step := (border_2.w-4) / qn
      
      if (d:=quest.get_current_question_index())>0 then
         progresslbl.set_size(string(integer(d*progress_step)), "100%-4")
      else
         progresslbl.set_size("2", "100%-4")
      
      label_11.set_label("Quest Progress("|| string(d) || "/" || string(qn) ||")")
      
      self.init()
      self.resize()

   end
   
