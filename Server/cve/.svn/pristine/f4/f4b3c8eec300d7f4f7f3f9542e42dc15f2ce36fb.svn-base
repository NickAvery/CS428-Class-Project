   method new_question(q)
      local d,  qn, ans
          
      if /q then fail
      status_lbl.set_attribs("bg=whitish blue")
      questiontl.set_contents([q])
      
      ans := quest.get_answer()     
      
      if quest.is_current_question_MC() then{
	 overlay_set.set_which_one(overlay_item_MC)
	 init_MC(ans)
         }
      else{
	 type_lst.set_selection(1)
	 overlay_set.set_which_one(overlay_item_short)
	 if \q then
	    answeretl.set_contents([ans])
         }
	 
      qn := quest.question_count()
      if qn>0 then
	 progress_step := (border_2.w-4) / qn
      else
	 progress_step := 1
	       
	    
      if (d:=quest.get_current_question_index())>0 then{
	 progresslbl.set_size(string(integer(d*progress_step)), "100%-4")
         }
	 
      qn := string(quest.question_count())
      label_11.set_label("Question Index ("|| string(d) || "/" || qn ||")")
      self.init()
      self.resize()
      #border_1.display()

   end
   
   method list_to_string(L)
      local s, s2
      s := ""
      every s2:=!L  do
	 s ||:=s2 || " "      
      return trim(s, ' \t')
   end
   
   method is_valid_question()
      local q, L, ans, s, new_QA, i
      
      q := list_to_string(questiontl.get_contents())
      

       if q=="" then{
	  status_lbl.set_attribs("bg=red")
	  status_lbl.set_label (  " Must have a valid question" )
	  status_lbl.resize()
	  fail
       }
       if q[-1]~=="?" then q ||:="?" 
      if type_lst.get_selection()> 1 then{	# MC or True/False
	 ans := 0
	 L:=[]
	 every i:=1 to *mc_list do{
	    s := list_to_string( mc_list[i].get_contents())
	    if s=="" then break
	    put(L, s)
	    if chk_list[i].is_checked() then{
	       ans := i
	       }
	       
	    }# every
	 if ans = 0 then{
	    status_lbl.set_attribs("bg=red")
	    status_lbl.set_label ( " Must have a valid answer  ")
	    status_lbl.resize()
	    fail
	    }
	    
	 new_QA := QuestionAnswer(q, ans, "MC", L)
         }
      else{ # short answer question
	 ans := list_to_string ( answeretl.get_contents() )
	 if ans=="" then{
	    status_lbl.set_attribs("bg=red")
	    status_lbl.set_label ( " Must have a valid answer" )
	    status_lbl.resize()
	    fail
	    }
	 new_QA := QuestionAnswer(q, ans, "SA")
         }
	 quest.set_current_question_answer(new_QA)
      return
   end

   method on_nextbtn(ev)
      local q
      
      is_valid_question() | fail
      
      q := quest.get_next_question()

      if /q  then
	 if nextbtn.is_unshaded() then nextbtn.set_is_shaded()
	    
      if previousbtn.is_shaded() then
	 previousbtn.clear_is_shaded()
	 
      if \q then
	 new_question(q)
   end

   method on_previousbtn(ev)
      local q
      
      is_valid_question() | fail
      
      q := quest.get_previous_question()
      if /q  then{
	 if previousbtn.is_unshaded() then previousbtn.set_is_shaded()
	 fail
      }
      
      if nextbtn.is_shaded() then
	 nextbtn.clear_is_shaded()	 
	 

      if \q then
	 new_question(q)
   end
   
