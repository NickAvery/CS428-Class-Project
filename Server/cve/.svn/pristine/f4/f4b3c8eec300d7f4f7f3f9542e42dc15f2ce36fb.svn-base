   method on_type_lst(ev)   
      case type_lst.get_selection() of{
	 1 : { overlay_set.set_which_one(overlay_item_short)}
	 2 : { 
	    visible_set_add(chk3)
	    visible_set_add(chk4)
	    visible_set_add(mc3_etl)
	    visible_set_add(mc4_etl)
	    mc1_etl.set_contents([""])
	    mc2_etl.set_contents([""])
	    mc3_etl.set_contents([""])
	    mc4_etl.set_contents([""])
	    overlay_set.set_which_one(overlay_item_MC)
	    }
	 3 : { 
	    visible_set_remove(chk3)
	    visible_set_remove(chk4)
	    visible_set_remove(mc3_etl)
	    visible_set_remove(mc4_etl)
	    mc1_etl.set_contents(["True"])
	    mc2_etl.set_contents(["False"])
	    mc3_etl.set_contents([""])
	    mc4_etl.set_contents([""])
	    overlay_set.set_which_one(overlay_item_MC) 
	    }
	 } # case
   end

   method on_create_btn(ev)
      local q
      case type_lst.get_selection() of{  # MC, True/False or SA
	 2: q := QuestionAnswer("",0, "MC",["", "", "", ""])
	 3: q := QuestionAnswer("",0, "MC",["True","False"])
	 default: q := QuestionAnswer("","", "SA")
      }# case
      
      quest.add_question_answer(q, 0)
      q := quest.get_next_question()
      
      if \q then
	 new_question(q)
   end

