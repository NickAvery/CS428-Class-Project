   method init_dialog()
   local cmp, cmpOpts, # identify the compiler and its options
         lnk, lnkOpts  # identify the linker and its options
      cmp := lnk := "g++"
      cmpOpts :="-O"
      lnkOpts :="-s"
      pname := file_name(projectname)

      if find(".uprj"|".icn", file_name(pname)) then {
	 pname ? {
	    p_name := tab(find("."))
	    move(1)
	    tab(0) # discard extension
	    }
         pname := p_name
	 }

      $ifdef _UNIX
         targetFile_name_txt.set_contents(pname)
      $else
         targetFile_name_txt.set_contents(pname||".exe")
      $endif
      Makefile_name_txt.set_contents(pname||"_uprjMakefile")
      compiler_txt.set_contents(cmp)
      compilerflags_txt.set_contents(cmpOpts)
      linker_txt.set_contents(lnk)
      linkerflags_txt.set_contents(lnkOpts)

      if TypeOfBuild === "Console" then {
         if Compiler === "Borland C" then {
	    cmp  := "BCC32"
	    lnk  := "g++"
	    cmpOpts  := "-q -O1 -v-"
	    lnkOpts  := "-v- -Tpe -ap -c"
	  }
         else {
	    cmp := lnk := "g++"
	    cmpOpts  := "-O"
	    lnkOpts  := "-s"
	  }
      $ifdef _UNIX
         targetFile_name_txt.set_contents(pname)
      $else
         targetFile_name_txt.set_contents(pname||".exe")
      $endif
         Makefile_name_txt.set_contents(pname||"_uprjMakefile")
         compiler_txt.set_contents(cmp)
         compilerflags_txt.set_contents(cmpOpts)
         linker_txt.set_contents(lnk)
         linkerflags_txt.set_contents(lnkOpts)
         needConsole  := "TRUE"
       }
      else if TypeOfBuild === "GUI" then {
         if Compiler === "Cygwin" then {
	    cmp := lnk := "g++"
	    cmpOpts  := "-O"
	    lnkOpts  := "-e _mainCRTStartup"
	  }
         else if Compiler === "Borland C" then {
	    cmp  := "BCC32"
	    lnk  := "g++"
	    cmpOpts  := "-O"
	    lnkOpts  := "-v- -Tpe -aa -c"
	  }
         else {
	    cmp:= lnk := "g++"
	    cmpOpts  := "-O"
	    lnkOpts  := "-s -mwindows"
	  }
	 #file_name(projectname[1:*projectname-3])
         targetFile_name_txt.set_contents(projectname || ".exe")
         Makefile_name_txt.set_contents(pname || "_uprjMakefile")
         compiler_txt.set_contents(cmp)
         compilerflags_txt.set_contents(cmpOpts)
         linker_txt.set_contents(lnk)
         linkerflags_txt.set_contents(lnkOpts)
         needConsole  := "FALSE"
       }
      else if TypeOfBuild === "Dynamic" then {
	 if Compiler === "MinGW" then {
	    cmp := lnk := "g++"
	    cmpOpts  := "-O"
	    lnkOpts  := "-s"
          }
         else if Compiler === "Cygwin" then {
	    cmp := lnk := "g++"
	    cmpOpts  := "-O"
	    lnkOpts  := "-s"
	  }
         else if Compiler === "Borland C" then {
	    cmp  := "BCC32"
	    lnk  := "g++"
	    cmpOpts := "-q -O1 -v-"
	    lnkOpts := "-v- -Tpe -aa -c"
	  }
         else {
	    cmp := lnk := "g++"
	  }

	 #file_name(projectname[1:*projectname-3])
         targetFile_name_txt.set_contents(projectname || ".dll")
         Makefile_name_txt.set_contents(pname || "_uprjMakefile")
         compiler_txt.set_contents(cmp)
         compilerflags_txt.set_contents(cmpOpts)
         linker_txt.set_contents(lnk)
         linkerflags_txt.set_contents(lnkOpts)
         needConsole  := "FALSE"
       }
      fileslist := []
      filesloclist := []
      libslist := []
      optslist := []
      incpathslist := []
      libpathslist := []
      userMakefilelist := []
      userTargetlist := []
      ruleslist := []
      UIDE_VERS := "0.1"
      makeName  :=  pname || "_uprjMakefile"
   end

   method on_ok_btn(ev)
      local cwd
      cwd := chdir()

      createMakefile(pname)
      chdir(filedirectory)

      if not find(".uprj", projectname) then
         view.uide.handle_openproject(ev, projectname||".uprj")
      view.FileBrowser.addreplace(file_name(projectname), "Editor", 0)
      self.dispose()
      chdir(cwd)
   end

   # createMakefile
   # Build the file name projectname+"_uprjMakefile" using
   # informations stored in project
   #
   method createMakefile(pname)
      local l := "", resName := "", f, i, objn, targetfile, isDLL, lim, needc,
        tfn := targetFile_name_txt.get_contents(),
        compilerName := compiler_txt.get_contents(), cwd

      cwd := chdir()

      if f := open(pname||"_uprjMakefile", "w") then {
	 #where to write uprjMakefile

	 if cc == "BCC32" then
	    isBorland := "TRUE"

	 write(f,"#", repl("=",55), "\n",
	       "#@V@:Note: File automatically generated by CVE IDE ",
	       UIDE_VERS, " (",compilerName,").\n",
	       "# Generated ",&clock," ",&date, "\n",
	       "#", repl("=",55), "\n",
	       "# Standard defines:")

	 if isBorland == "TRUE" then {
$ifdef _MS_WINDOWS
	    write(f,".AUTODEPEND\n",
		  "# Borland C++ tools\n",
		  "IMPLIB\t= $(BCCROOT)\\bin\\Implib\n",
		  "ILINK32\t= $(BCCROOT)\\bin\\ILink32\n",
		  "TLIB\t= $(BCCROOT)\\bin\\TLib\n",
		  "BRC32\t= $(BCCROOT)\\bin\\Brc32\n",
		  "TASM32\t= $(BCROOT)\\bin\\Tasm32\n",
		  "CC  \t= $(BCCROOT)\\bin\\\n",
		  cc,"\n")
$endif
	    }
	 else {		#gnu stuff
	    write(f,"CC  \t=\t",compilerName, "\n",
		  "LD  \t=\t",linker_txt.get_contents(), "\n",
		  "WRES\t=\twindres")
	    if compilerName == "g++" then
	       write(f, "CCOMP\t=\tgcc")
        }

     ObjDir := objpath_txt.get_contents()
     if ObjDir == "" then
        ObjDir := "."

     BinDir := binpath_txt.get_contents()
     if BinDir == "" then
        BinDir := "."

     write(f, "oDir\t=\t", ObjDir, "\n",
	   "Bin\t=\t", BinDir, "\n",
	   "Src\t=\t", SrcDir)

     writes(f,"libDirs\t=")
     if isBorland == "TRUE" & *libpathslist > 0 then
	writes(f,"\t-L")

     every i := 1 to *libpathslist do {
        if isBorland == "TRUE" then {
	   writes(f, map(libpathslist[i]))
           if i < *libpathslist then
	      writes(f,";")
	   }
        else {
	   writes(f,"\t-L", map(libpathslist[i]))
 	   if i < *libpathslist then
	      writes(f,"\\")
	   }
	write(f)
        }

     write(f)
     if isBorland == "TRUE" then
	write(f)

     writes(f,"incDirs\t=")
     if isBorland == "TRUE" & *libpathslist > 0 then
	writes(f,"\t-I")

     every i := 1 to *incpathslist do {
        if isBorland == "TRUE" then {
	   writes(f, map(incpathslist[i]))
           if i < *incpathslist then
	      writes(f,";")
	   }
        else {
	   writes(f,"\t-L", map(incpathslist[i]))
 	   if i < *incpathslist then
	      writes(f,"\\")
	   }
	write(f)
        }

      write(f)

      #ld flags
      write(f,"LD_FLAGS =\t",linkerflags_txt.get_contents())
      #LIBS switch
      if isBorland == "FALSE" then {
	 writes(f,"LIBS\t=\t")
	 l := getLibFiles(f)
	 write(f,l)
         }

      #C_FLAGS & paths to includes
      writes(f,"C_FLAGS\t=\t", compilerflags_txt.get_contents())
      every i := 1 to *optslist do {
 	 write(f,"\\")
	 writes(f,"\t", map(optslist[i]))
	 }
      write(f,"\n")

      #sources
      writes(f,"SRCS\t=")
      l := getSourceFiles(f)
      write(f,l)

      #objects
      writes(f,"EXOBJS\t=")
      every i := 1 to *fileslist do {
         if find(".cpp", fileslist[i]) then
         if isBorland == "TRUE" then {
            objn := srcN2ObjN(fileslist[i],".obj")
            write("objn : ",objn)
	    writes(f,"\\\n\t$(oDir)\\",objn)
            if find(".res", objn) then {
	       resName := "$(oDir)\\" || objn
	       }
	    else {
	       resName ||:= "+$(oDir)\\" || objn
	       }
            }
         else {
	    writes(f,"\\")
            objn := srcN2ObjN(fileslist[i],".o")
	    write(f)
	    writes(f,"\t$(oDir)/",objn) #file[1:*projectname-3]||".o")
            }
	 }
      write(f,"\n\nALLOBJS\t=\t$(EXOBJS)")
      targetfile := tfn
      if isBorland == "TRUE" then {
	 write(f,"BCCJUNK\t=\t*.il* *.csm *.tds\n",
	       "ALLBIN\t=\t$(Bin)\\", file_name(targetfile), "\n",
	       "ALLTGT\t=\t$(Bin)\\", file_name(targetfile))
         }
      else {
	 if find(".dll",file_name(targetfile)) then {
	    writes(f,"ALLBIN\t=\t$(Bin)/", file_name(targetfile))
	    writes(f," $(Bin)/", file_name(targetfile,".a"))
	    }
	 else
	    write(f,"ALLBIN\t=\t$(Bin)/", file_name(targetfile))
	 write(f,"ALLTGT\t=\t$(Bin)/", file_name(targetfile))
	 }

      #Options
      write(f,"# User defines:")

      write(f,"#@# Targets follow ---------------------------------\n\n")
      write(f,"all:\t$(ALLTGT)\n\n")
      write(f,"objs:\t$(ALLOBJS)\n\n")
      write(f,"cleanobjs:\n\trm -f $(ALLOBJS)\n\n")
      if isBorland == "TRUE" then
	 write(f,"cleanbin:\n\trm -f $(ALLBIN) $(BCCJUNK)\n\n")
      else
	 write(f,"cleanbin:\n\trm -f $(ALLBIN)\n\n")
      write(f,"clean:\tcleanobjs cleanbin\n\n")
      write(f,"cleanall:\tcleanobjs cleanbin\n\n")

      write(f,"#@# User Targets follow ---------------------------------\n\n")

      write(f,"#@# Dependency rules follow -----------------------------")

      #allow user to override the all: target with a #all comment
      if not (find("#all",userTargetlist[1])) then {
         isDLL := "FALSE"
$ifdef _MS_WINDOWS
	 if find(".dll", tfn) then
	 isDLL := "TRUE"
$endif

         if isBorland == "TRUE" then {
$ifdef _MS_WINDOWS #implies Windows for now
	       write(f,"$(Bin)\\", file_name(tfn), " : $(EXOBJS)")

               #standard linker commands
	       write(f,"  $(ILINK32) @&&|\n $(LD_FLAGS) $(libDirs) +")
	       write(f,"$(BCC32STARTUP)")
  	       every i := 1 to *fileslist do {
 	          objn := srcN2ObjN(fileslist[i],".obj")
		  if find(".res", objn) then {
		     write(f," +\n$(oDir)\\",srcN2ObjN(fileslist[i],".obj"))
		     }
		  }
	       write(f,"\n$<,$*\n")

	       #OK, scan the libs list to output BCC lib files
	       every i := 1 to *libslist do {
		  write(f," +")
		  if find(".lib", map(libslist[i])) then
		     write(f,libslist[i])  #already has .lib
		  else
		     write(f,libslist[i], ".lib")
		  }
		write(f)
		write(f,"$(BCC32DEF)")  #allow DEF files!

            if find(".res", objn) then {   #if (*resName) had a .res file
	       write(f, resName||"\n\n|\n")
	     }
	    else write(f, "\n\n|\n")
           $endif
         }
        else {   #gnu compatible
          $ifdef _MS_WINDOWS
           if isDLL == "TRUE" then {
	      write(f,"$(Bin)/",file_name(tfn), ": $(EXOBJS)")
	      write(f,"\t$(CC) -shared -o $(Bin)/", file_name(tfn)," \\")
	      write(f,"\t-Wl,--out-implib=$(Bin)/", file_name(tfn),".a \\")
	      write(f,"\t-Wl,--export-all-symbols " ||
		    "-Wl,--enable-auto-import -Wl,--strip-all \\")
	      write(f,"\t-Wl,--whole-archive $(EXOBJS) \\")
	      write(f,"\t-Wl,--no-whole-archive $(LIBS)")
	    }
           else
	  $endif
	    {
	      write(f,"$(Bin)/",file_name(tfn), ": $(EXOBJS)")
	      writes(f,"\t$(LD) -o $(Bin)/", file_name(tfn))
	      write(f," $(EXOBJS) $(incDirs) $(libDirs) $(LD_FLAGS) $(LIBS)")
	    }
	 }

     } #first if

    #now the src Files

    if isBorland == "TRUE" then {
       #now count on autodependency for borland
       every i := 1 to *fileslist do {
	    writes(f,"$(oDir)\\",srcN2ObjN(fileslist[i],".obj"))
	    write(f," : ",catSrc("$(Src)\\", fileslist[i]))

	    # check for .rc files
	    if find(".rc"|".RC",fileslist[i]) then {
	        write(f,"  $(BRC32) -r $(incDirs) -fo$@ ",catSrc("$(Src)\\",
		      fileslist[i]))
	      }
	    else {		#it is a .rc file
	        write(f,"  $(CC) -c $(C_FLAGS) $(incDirs) -o$@ ",
		      catSrc("$(Src)\\",fileslist[i]))
	      }
	}
      }
    else {  #NOT Borland
	every i := 1 to *fileslist do {
	   # make GUI more responsive here
	   $ifdef _MS_WINDOWS
	    if find(".rc"|".RC",fileslist[i]) then {
		write(f,"\n$(oDir)/",srcN2ObjN(fileslist[i],".o"))
		write(f," : " || catSrc("$(Src)/",fileslist[i]))
		write(f,"\t$(WRES) " || catSrc("$(Src)/",fileslist[i]))
		write(f," $(oDir)/" || srcN2ObjN(fileslist[i],".o"))
		write(f)
	      }
	   else
          $endif

	    # bogus: findRule called but its result is not used.
	    if needCFile(fileslist[i]) == "TRUE" then { # mix c++/c?
	       findRule(file_name(fileslist[i]),"gcc")
	       }
	    else {
	       findRule(file_name(fileslist[i]), cc)
	       }
	    }

	 lim := *ruleslist
         every i := 1 to lim do {
	    needc := needCFile(ruleslist[i]) #C++/c mix?

	    write(f,"\n$(oDir)/",ruleslist[i])
	    #check for continuation lines
	    if needc == "TRUE" then
	       write(f,"\t$(CCOMP) $(C_FLAGS) $(incDirs) -c -o $@ $<\n")
	    else
	       write(f,"\t$(CC) $(C_FLAGS) $(incDirs) -c -o $@ $<\n")
	  }
        }

    buffer := makeName || " ("||&clock||" "||&date||") - Makefile saved."
#    view.uideMsgBox.set_contents(buffer)
    close(f)
    }

    chdir(filedirectory)
    if f := open(pname||".uprj", "w") then {#where to write project file info.

    write(f,"#", repl("=",55))
    writes(f,"#@V@:Note: C/C++ Project File generated by CVE IDE ")
    write(f,UIDE_VERS, " (",\compilerName,").")
    write(f,"# Generated ",&clock," ",&date)
    write(f,"#", repl("=",55))
    write(f)
    write(f,"Project Name    : ",pname,".uprj")
    write(f,"Files Directory : ",filedirectory)
    write(f)
    write(f,"Project Files List:")
      every i := 1 to *fileslist do
	 write(f, "\t - "||fileslist[i])
    write(f,"#", repl("=",55))
    close(f)
    }
    chdir(cwd)
   end
   #
   # needCFile
   #
   method needCFile(cF)
      if cc ~== "g++" then     #can't "need" if not g++
	 return "FALSE"
      if map(cF[-2:0]) == ".c" then #must be .c only
	 return "TRUE"
      return "FALSE"
   end

   #
   # findRule finds the rules to build object file pointed by name
   #
   method findRule(name, comp)
   local i, incdir
     # add include directories
     every i := 1 to *incpathslist do {
        incdir := incpathslist[i]
        comp ||:= " -I" || incdir
        }
     # include any command-line defines
     return comp || " " || cFlags || " -MM " || fileQuote (name)
   end

   #
   # fileQuote
   #
   method fileQuote(fname)
      # adds quotes to file name on windows
$ifdef _MS_WINDOWS
      if find(" ", fname) then {
         return "\"" || fname || "\""
         }
      else
$endif
      return fname
   end

   #
   # catSrc
   #
   method catSrc (src,file)
      # allow user to use something other than $(Src) for source files.
      local srcfile

$ifdef USE_SRC_INMAKE
      if file[1] == "=" then
	 return file[2:0]
      srcfile := src || file
      return srcfile
$else
      if file[1] == "=" then
	 return file[2:0]
      return file
$endif
   end

   #
   # srcN2ObjN
   #
   method srcN2ObjN(name, ext)
      #replace .cpp with .o
      local curExt, objn, i
      objn := file_name(name[1:*name-3])  #||".o"
      every i := 1 to *name do {
	 if name[i] == "." then {
	    curExt := name[i+1 : *name]
	    }
	 }
$ifdef _MS_WINDOWS #implies Windows for now
      if curExt == "rc" then {
	 if cc == "BCC32" then objn ||:= ".res"	 #borland uses .res
         else objn ||:= ".res"  #gcc uses .o
         }
      else
$endif
	 objn ||:= ext
      return objn
   end
   #
   # getLibFiles
   #
   method getLibFiles(f)
   local i
      #
      # This will take the list of libraries in the libFiles list and
      # convert them to a string of -l's with the lib part strip suitable
      # for gnu ld input.
      #
      every i := 1 to *libslist do {
	 writes(f," -l", map(libslist[i]))
	 }
      write(f, "\n")
   end

   #
   # getSourceFiles
   #
   method getSourceFiles(f)
   local i
      # Take the list of files in the SourceFiles list and convert them to a
      # string with the SRC part strip suitable for gnu ld input.
      every i := 1 to *fileslist do {
	 writes(f, "\\\n\t$(Src)\t", map(fileslist[i]))
	 }
   end
   #
   # txtDialog
   #
   method txtDialog(text, inputstr : "")
      local oldwindow
      oldwindow := &window
      &window := win
         TextDialog(text,,inputstr,18)
         inputstr := dialog_value[1]
      &window := oldwindow
      return inputstr
   end


