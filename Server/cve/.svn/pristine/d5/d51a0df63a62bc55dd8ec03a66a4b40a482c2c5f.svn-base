   method setup()
      local addincpath_btn, addlibpath_btn, binPath_btn, defines_tab, dellibpath_btn, files_tab, label_10, label_11, label_12, label_13, libraries_tab, libraryfiles_lbl, names_lbl1, names_lbl2, names_lbl3, names_lbl4, names_lbl5, names_lbl6, names_tab, objPath_btn, ok_btn, options_Add, options_Del, paths_tab, sourcefiles_lbl
      self.set_attribs("size=414,308", "font=serif,bold,14", "label=Project Editor", "bg=light gray")
      ProjEditor_TabSet := TabSet()
      ProjEditor_TabSet.set_pos("5", "5")
      ProjEditor_TabSet.set_size("100%-10", "100%-35")
      ProjEditor_TabSet.set_attribs("font=serif,14,bold")
      names_tab := TabItem()
